{
  "manifest_version": 2,

  "name": "SMS Tsu Beautifier",
  "description": "This extension changes SMS Tsu layout into the more user-friendly one",
  "version": "1.0",

  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["http://sms.tsu.ge/sms/Students/Cxrilebi.aspx"],
      "js": ["js/jquery-2.2.1.min.js", "js/scheduleParser.js", "js/schedulePage.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "templates/*",
    "css/*",
    "js/*",
    "html/*"
  ]
}
