{
  "manifest_version": 2,

  "name": "SMS Tsu Beautifier",
  "description": "This extension changes SMS Tsu layout into the more user-friendly one",
  "version": "1.0",

  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.google.ge/"],
      "js": ["js/jquery-2.2.1.min.js", "js/script.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "templates/*",
    "css/*",
    "js/*"
  ]
}
